<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TercaLivreApp.Views.SitePage"
             Title="{Binding Title}"
             xmlns:local="clr-namespace:TercaLivreApp.ViewModels"  
             xmlns:model="clr-namespace:TercaLivreApp.Models"  
             x:Name="BrowseSitePage">    
    <StackLayout>        
        <WebView Source="https://tercalivre.com.br/" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" ></WebView>
    </StackLayout>    
</ContentPage>
